
.uni-navbar {
  display: block;
  position: relative;
  width: 100%;
  background-color: #FFFFFF;
  overflow: hidden;
}
.uni-navbar-shadow {
  box-shadow: 0 1px 6px #ccc;
}
.uni-navbar.uni-navbar-fixed {
  position: fixed;
  z-index: 9;
}
.uni-navbar-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  width: 100%;
  height: 2.75em;
  line-height: 2.75em;
  font-size: 16px;
}
